        "encrypted_text": text;
    };

service : {
    "getDevices": (Principal) -> (vec record {
                                        DeviceAlias;
                                        PublicKey;
                                }) query;
    "registerDevice": (Principal, DeviceAlias, PublicKey) -> (bool);
    "deleteDevice": (Principal, DeviceAlias)
}
